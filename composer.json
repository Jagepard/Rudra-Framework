{
    "name"              : "rudra/framework",
    "description"       : "Rudra framework",
    "type"              : "library",
    "keywords"          : [
        "rudra",
        "framework",
        "mvc-framework",
        "mvc-pattern",
        "mvc-architecture",
        "mvc"
    ],
    "license"           : "GPL-3.0-or-later",
    "authors"           : [
        {
            "name"     : "Danila Korotkov (Jagepard)",
            "email"    : "jagepard@yandex.ru",
            "homepage" : "https://jagepard.ru",
            "role"     : "Developer"
        }
    ],
    "support"           : {
        "email" : "jagepard@yandex.ru",
        "skype" : "jagepard.ru"
    },
    "autoload"          : {
        "psr-4" : {
            "App\\" : "app/"
        }
    },
    "require"           : {
        "php"               : ">=7.1",
        "twig/twig"         : "~1.0",
        "kint-php/kint"     : "^1.0",
        "maximebf/debugbar" : "1.*",
        "symfony/yaml"      : "*",
        "robmorgan/phinx"   : "^0.9.1",
        "fzaninotto/faker"  : "^1.7",
        "rudra/connector"   : "self.version",
        "rudra/exception"   : "self.version",
        "rudra/controller"  : "self.version",
        "rudra/auth"        : "self.version",
        "rudra/pagination"  : "self.version",
        "rudra/router"      : "self.version",
        "rudra/container"   : "self.version",
        "rudra/validation"  : "self.version",
        "rudra/redirect"    : "self.version",
        "rudra/uri"         : "self.version",
        "rudra/model"       : "self.version",
        "rudra/annotation"  : "self.version"
    },
    "minimum-stability" : "dev",
    "config"            : {
        "optimize-autoloader" : true,
        "prefer-stable"       : true
    }
}
